{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/user(.*)",
      "dest": "src/index.js"
    },
    {
      "src": "/api/games(.*)",
      "dest": "src/index.js"
    },
    {
      "src": "/api/test(.*)",
      "dest": "src/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ]
}
